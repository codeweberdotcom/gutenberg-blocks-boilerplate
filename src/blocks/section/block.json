        "backgroundImageSize": {
            "type": "string",
            "default": ""
        },
        "backgroundSize": {
            "type": "string",
            "default": ""
        },
        "backgroundVideoId": {
            "type": "number",
            "default": 0
        },
        "backgroundVideoUrl": {
            "type": "string",
            "default": ""
        },
        "textColor": {
            "type": "string",
            "default": "none"
        },
        "backgroundOverlay": {
            "type": "string",
            "default": ""
        },
        "backgroundPatternUrl": {
            "type": "string",
            "default": ""
        },
        "containerClass": {
            "type": "string",
            "default": "py-14 py-md-16"
        },
        "containerType": {
            "type": "string",
            "default": "container"
