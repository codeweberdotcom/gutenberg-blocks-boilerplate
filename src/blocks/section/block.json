        "spacingMd": {
        	"type": "string",
        	"default": ""
        },
        "spacingLg": {
        	"type": "string",
        	"default": ""
        },
        "spacingXl": {
        	"type": "string",
        	"default": ""
        },
        "spacingXxl": {
        	"type": "string",
        	"default": ""
        }
       },
    "textdomain": "naviddev-gutenberg-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
