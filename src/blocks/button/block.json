{
    "apiVersion": 2,
    "name": "naviddev-gutenberg-blocks/button",
    "version": "0.1.0",
    "title": "Button",
    "category": "naviddev-gutenberg-blocks",
    "icon": "calendar-alt",
    "description": "Example block written with ESNext standard and JSX support build step required.",
    "supports": {
        "html": false,
        "anchor": true
    },
    "attributes": {
        "content": {
            "type": "string",
            "default": "Hello World!"
        },
        "buttonText": {
            "type": "string",
            "default": "Large Button"
        },
        "buttonSize": {
            "type": "string",
            "default": "medium"
        },
        "buttonColor": {
            "type": "string",
            "default": "primary"
        },
        "buttonStyle": {
            "type": "string",
            "default": "solid"
        },
        "buttonType": {
            "type": "string",
            "default": "solid"
        },
        "buttonIconPosition": {
            "type": "string",
            "default": "left"
        },
        "iconClass": {
            "type": "string",
            "default": "uil-angle-right"
        },
        "socialIcon": {
            "type": "string",
            "default": "facebook"
        },
        "socialIconStyle": {
            "type": "string",
            "default": "style1"
        }
    },
    "textdomain": "naviddev-gutenberg-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
