{
    "apiVersion": 2,
    "name": "naviddev-gutenberg-blocks/button2",
    "version": "0.1.0",
    "title": "Button2",
    "category": "naviddev-gutenberg-blocks",
    "icon": "calendar-alt",
    "description": "Example block written with ESNext standard and JSX support build step required.",
    "supports": {
        "html": false,
        "anchor": false
    },
    "attributes": {
        "buttonText": {
            "type": "string",
            "default": "Large Button"
        },
        "buttonSize": {
            "type": "string",
            "default": "btn-sm"
        },
        "buttonColor": {
            "type": "string",
            "default": "primary"
        },
        "buttonGradientColor": {
            "type": "string",
            "default": "gradient gradient-1"
        },
        "buttonStyle": {
            "type": "string",
            "default": "solid"
        },
        "buttonType": {
            "type": "string",
            "default": "solid"
        },
        "buttonShape": {
            "type": "string",
            "default": "rounded-0"
        },
        "buttonIconPosition": {
            "type": "string",
            "default": "left"
        },
        "iconClass": {
            "type": "string",
            "default": "uil-angle-right"
        },
        "socialIconClass": {
            "type": "string",
            "default": "facebook"
        },
        "socialIconStyle": {
            "type": "string",
            "default": "style_1"
        },
        "LinkType": {
            "type": "string",
            "default": "external"
        },
        "LinkUrl": {
            "type": "string",
            "default": "javascript:void(0)",
			"format": "uri"
        }
    },
    "textdomain": "naviddev-gutenberg-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
